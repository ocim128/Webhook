{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "public-dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/webhooks/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/hooks/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/meta",
      "destination": "/api/index.js"
    },
    {
      "source": "/health",
      "destination": "/api/index.js"
    },
    {
      "source": "/((?!assets/|favicon.ico|.*\\.js|.*\\.css|.*\\.png|.*\\.svg).*)",
      "destination": "/api/index.js"
    }
  ]
}